class Shape:
    def area(self):
        pass

class Rectangle(Shape):
    def __init__(self, width, height):
        self.width = width
        self.height = height
    
    def area(self):
        return self.width * self.height

class Square(Shape):
    def __init__(self, size):
        self.size = size
    
    def area(self):
        return self.size * self.size

# Função que calcula a soma das áreas de várias figuras
def sum_of_areas(figures):
    total_area = 0
    for figure in figures:
        total_area += figure.area()
    return total_area

# Teste após a modificação
rectangle = Rectangle(3, 4)
square = Square(5)
figures = [rectangle, square]
print("Soma das áreas (depois):", sum_of_areas(figures))
